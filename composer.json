{
  "name": "bizley/deep-instantiate",
  "description": "Deep Instantiator for Yii 2.",
  "type": "yii2-extension",
  "keywords": ["yii2", "di", "dependency injection"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "PaweÅ‚ Bizley Brzozowski",
      "email": "pawel@positive.codes"
    }
  ],
  "support": {
    "source": "https://github.com/bizley/yii2-deep-instantiate"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/bizley"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "^2.0.11"
  },
  "require-dev": {
    "infection/infection": "*",
    "phpstan/phpstan": "*",
    "phpunit/phpunit": "^6.5",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Bizley\\DeepInstantiate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bizley\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
